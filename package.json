    "lint-staged": "^15.2.7",
    "pinia": "^2.2.0",
    "typescript-eslint": "^7.18.0",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.2.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.3",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vue-tsc": "^2.0.24"
  }
